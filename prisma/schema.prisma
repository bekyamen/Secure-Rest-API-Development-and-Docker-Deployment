generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int        @id @default(autoincrement())
  email     String     @unique
  password  String
  customers Customer[]
}

model Customer {
  id     Int    @id @default(autoincrement())
  name   String
  phone  String?
  userId Int
  user   User   @relation(fields: [userId], references: [id])
}
